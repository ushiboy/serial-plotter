{
  "name": "serial-plotter",
  "version": "0.1.0",
  "description": "serial-plotter",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts .",
    "format": "npm run format-js && npm run format-css",
    "format-js": "prettier --write 'src/**/*.(t|j)s?(x)'",
    "format-css": "prettier --write 'src/**/*.scss'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ushiboy/serial-plotter.git"
  },
  "author": "ushiboy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ushiboy/serial-plotter/issues"
  },
  "homepage": "https://github.com/ushiboy/serial-plotter#readme",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "ex_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack/main.config.js",
            "renderer": {
              "config": "./webpack/renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer/index.tsx",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "autoprefixer": "^10.2.3",
    "css-loader": "^5.0.1",
    "electron": "^11.2.0",
    "electron-rebuild": "^2.3.4",
    "eslint": "^7.18.0",
    "eslint-plugin-import": "^2.22.1",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "mini-css-extract-plugin": "^1.3.4",
    "node-loader": "^1.0.2",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.2.0",
    "precss": "^4.0.0",
    "prettier": "^2.2.1",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "serialport": "^9.0.6"
  }
}
